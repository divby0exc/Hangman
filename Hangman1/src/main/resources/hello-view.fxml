            <Button fx:id="N" text="N" GridPane.rowIndex="1" GridPane.columnIndex="4" onAction="#getID"/>
            <Button fx:id="O" text="O" GridPane.rowIndex="1" GridPane.columnIndex="5" onAction="#getID"/>
            <Button fx:id="P" text="P" GridPane.rowIndex="1" GridPane.columnIndex="6" onAction="#getID"/>
            <Button fx:id="Q" text="Q" GridPane.rowIndex="1" GridPane.columnIndex="7" onAction="#getID"/>
            <Button fx:id="R" text="R" GridPane.rowIndex="1" GridPane.columnIndex="8" onAction="#getID"/>
            <Button fx:id="S" text="S" GridPane.rowIndex="2" GridPane.columnIndex="0" onAction="#getID"/>
            <Button fx:id="T" text="T" GridPane.rowIndex="2" GridPane.columnIndex="1" onAction="#getID"/>
            <Button fx:id="U" text="U" GridPane.rowIndex="2" GridPane.columnIndex="2" onAction="#getID"/>
            <Button fx:id="V" text="V" GridPane.rowIndex="2" GridPane.columnIndex="3" onAction="#getID"/>
            <Button fx:id="W" text="W" GridPane.rowIndex="2" GridPane.columnIndex="4" onAction="#getID"/>
            <Button fx:id="X" text="X" GridPane.rowIndex="2" GridPane.columnIndex="5" onAction="#getID"/>
            <Button fx:id="Y" text="Y" GridPane.rowIndex="2" GridPane.columnIndex="6" onAction="#getID"/>
            <Button fx:id="Z" text="Z" GridPane.rowIndex="2" GridPane.columnIndex="7" onAction="#getID"/>

</GridPane>
