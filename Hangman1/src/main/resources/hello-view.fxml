<GridPane fx:controller="com.example.hangman1.Controller"
            xmlns:fx="http://javafx.com/fxml"
            style="-fx-border-color: red; -fx-border-width: 3; -fx-border-style: dashed; -fx-alignment: bottom-center">
            <padding>
                <Insets bottom="5"/>
            </padding>
            <Button fx:id="A" text="A" GridPane.rowIndex="0" GridPane.columnIndex="0" onAction="#getID"/>
            <Button fx:id="B" text="B" GridPane.rowIndex="0" GridPane.columnIndex="1" onAction="#getID"/>
